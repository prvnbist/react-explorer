{
   "name": "react-explorer",
   "version": "0.1.0",
   "author": {
      "name": "Praveen Bisht",
      "email": "prvnbist@gmail.com",
      "url": "https://twitter.com/prvnbist"
   },
   "description": "A file explorer built with React, Emotion & TailwindCSS",
   "repository": {
      "url": "https://github.com/prvnbist/react-explorer"
   },
   "bugs": {
      "url": "https://github.com/prvnbist/react-explorer"
   },
   "scripts": {
      "start": "react-scripts start",
      "build": "react-scripts build",
      "test": "react-scripts test --env=jsdom",
      "eject": "react-scripts eject",
      "format": "prettier --write \"**/*.{js,jsx,json,md}\""
   },
   "dependencies": {
      "@emotion/core": "^10.0.28",
      "@emotion/styled": "^10.0.27",
      "react": "^16.13.1",
      "react-dom": "^16.13.1",
      "react-icons": "^3.11.0",
      "react-scripts": "^3.4.1",
      "twin.macro": "^1.10.0"
   },
   "devDependencies": {
      "prettier": "^1.19.1"
   },
   "browserslist": {
      "production": [
         ">0.2%",
         "not dead",
         "not op_mini all"
      ],
      "development": [
         "last 1 chrome version",
         "last 1 firefox version",
         "last 1 safari version"
      ]
   },
   "babelMacros": {
      "twin": {
         "config": "tailwind.config.js",
         "preset": "emotion",
         "debugProp": true,
         "debugPlugins": false,
         "debug": false
      }
   }
}
